          template: servers
          bfd: eth11
{% endfor %}
{% for n in range(1,256,4) %}
        - name: to_server02_3_{{n}}
          ipv4: "10.252.203.{{n}}"
          asn: 650201
          template: servers
          bfd: eth11
{% endfor %}
{% for n in range(1,256,4) %}
        - name: to_server02_4_{{n}}
          ipv4: "10.252.204.{{n}}"
          asn: 650201
          template: servers
          bfd: eth11
{% endfor %}
        - name: to_leafB
