        - name: to_server02_1_{{n}}
          ipv4: "10.252.201.{{n}}"
          asn: 650201
          template: servers
          bfd: eth11
{% endfor %}
{% for n in range(1,256,4) %}
        - name: to_server02_2_{{n}}
          ipv4: "10.252.202.{{n}}"
