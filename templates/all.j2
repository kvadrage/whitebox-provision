  leaf01:
    router_id: "10.255.0.1"
    vrfs:
{% for n in vrfs_range %}
{% set vrf = vrfs.prefix + n|string%}
      - name: {{ vrf }}
        table: {{ n }}
{% endfor %}
    interfaces:
      loopback:
        lo:
          ipv4_addresses: ["127.0.0.1/8", "10.255.0.1/32"]
{% for n in vrfs_range %}
{% set vrf = vrfs.prefix + n|string%}
        {{vrf}}:
          ipv4_addresses: ["10.255.{{n}}.1/32"]
          vrf: {{vrf}}
{% endfor %}
      common:
        # to server01 eth11
        swp5s0:
          autoneg: True
          speed: 25000
          ipv4_addresses: ["10.252.1.1/30"]
          vrf: vrf1
          bfd: True

        # to server02 eth11
        swp5s1:
          autoneg: True
          speed: 25000
          ipv4_addresses: ["10.252.2.1/30"]
          vrf: vrf2
          bfd: True

        # to server sdn1 eth11
