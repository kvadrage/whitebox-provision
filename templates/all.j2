          speed: 100000
          ipv4_addresses: ["10.252.1.101/30"]
          vrf: vrf1
          bfd: True

        # to spine01 swp1
        swp1:
          autoneg: False
          speed: 100000
          ipv4_addresses: ["10.252.0.18/30"]
          bfd: True

{% for n in vrfs_range %}
{% set vrf = vrfs.prefix + n|string%}
        swp1.{{ vrfs.offset + n }}:
          ipv4_addresses: ["10.252.{{n}}.18/30"]
          vrf: {{vrf}}
